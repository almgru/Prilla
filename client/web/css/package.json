{
  "name": "prilla-frontend-css",
  "version": "0.1.1",
  "description": "Styling and layout for prilla frontend",
  "scripts": {
    "build": "sass --load-path ./node_modules/reset-css/ ./src/entry.scss:./out/prilla.css",
    "watch": "sass --load-path ./node_modules/reset-css/ --watch --poll ./src/entry.scss:./out/prilla.css",
    "serve": "http-server ./out -p 8082 -c-1",
    "dev": "npm-run-all --parallel watch serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/almgru/prilla.git"
  },
  "author": "Daniel Alm Grundstr√∂m <daniel.alm.grundstrom@protonmail.com> (https://github.com/almgru)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/almgru/prilla/issues"
  },
  "homepage": "https://github.com/almgru/prilla#readme",
  "devDependencies": {
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "reset-css": "^5.0.2",
    "sass": "^1.64.1"
  }
}
